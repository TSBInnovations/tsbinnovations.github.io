{
  "name": "tsbinnovations",
  "version": "0.1.1",
  "private": true,
  "homepage": "https://tsbinnovations.com",
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
